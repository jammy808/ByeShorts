{
    "manifest_version": 3,
    "name": "ByeShorts",
    "version": "1.0",
    "description": "Hide YouTube Shorts from your view.",
    "permissions": ["scripting", "tabs"],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "16": "icon.png"
    }
}
  